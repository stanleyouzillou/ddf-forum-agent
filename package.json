    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "playwright": "^1.55.0",
    "playwright-bdd": "^7.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
