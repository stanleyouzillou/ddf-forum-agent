  "main": "index.js",
  "scripts": {
    "start": "next start -p 3000",
    "dev": "next dev -p 3000",
    "test:unit": "jest --config jest.unit.config.js",
    "playwright:install": "pnpm exec playwright install",
    "playwright:install-deps": "pnpm dlx playwright install-deps",
    "playwright:install-ci": "pnpm dlx playwright install --with-deps",
    "test:e2e": "pnpm exec playwright test --project=e2e",
    "test:bdd:gen": "pnpm exec playwright-bdd gen --config=playwright.config.ts",
    "test:bdd": "pnpm run test:bdd:gen && pnpm exec playwright test --project=bdd",
    "test:all": "pnpm run test:bdd:gen && pnpm exec playwright test",
    "playwright:report": "pnpm exec playwright show-report"
  },
